    "ts-standard": "12.0.2"
  },
  "dependencies": {
    "typescript": "5.3.3",
    "cors": "2.8.5",
    "express": "4.18.2",
    "morgan": "^1.10.0"
  },
  "eslintConfig": {
    "parserOptions": {
